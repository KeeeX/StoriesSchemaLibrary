  "$id": "lotPart",
  "type": "object",
  "title": "The part of a lot",
  "required": [
    "reference",
    "resourceType",
    "lot"
  ],
  "additionalProperties": false,
  "properties": {
    "reference": {
      "$id": "#/properties/reference",
      "type": "string",
      "title": "The reference"
    },
    "resourceType": {
      "$id": "#/properties/resourceType",
      "type": "string",
      "title": "The type of resource"
    },
    "lot": {
      "$id": "#/properties/lot",
