{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "properties",
  "type": "object",
  "title": "The properties of an object",
  "required": [],
  "additionalProperties": true,
  "properties": {
    "address": {
      "$id": "#/properties/address",
      "type": "string",
      "title": "The exploitation address"
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The object description"
    },
    "geolocation": {
      "$id": "#/properties/geolocation",
      "type": "object",
      "title": "Geographic coordinate of the object",
      "properties": {
        "latitude": {
          "$id": "#/properties/geolocation/latitude",
          "type": "string",
          "title": "Latitude coordinate"
        },
        "longitude": {
          "$id": "#/properties/geolocation/longitude",
          "type": "string",
          "title": "Longitude coordinate"
        }
      },
      "examples": [
        {
          "latitude": "20.984696",
          "longitude": "105.853550"
        }
      ]
    },
    "links": {
      "$id": "#/properties/links",
      "type": "object",
      "title": "IDX links to other objects",
      "examples": [
        {
          "lot": "xodil-hucof-lukup-dydyl-vyles-fysud-vyhan-zezek-mofuh-geciv-rotyh-runyl-somez-zetob-nitug-valam-loxix",
          "exploitation": "xeboz-nehyb-nabar-hocut-rurut-cokik-kataz-nuneg-tysyp-kodun-pygit-mafur-hofah-pynos-ritef-gizip-coxix"
        }
      ]
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The object name",
      "examples": [
        "Exploitation 001",
        "Poulet lot 001"
      ]
    },
    "resourceType": {
      "$id": "#/properties/resourceType",
      "type": "string",
      "title": "The type of resource",
      "examples": [
        "Poulet PAC",
        "Pâtes",
        "Légumes secs",
        "Patate",
        "Fromage",
        "Vin"
      ]
    }
  }
}
